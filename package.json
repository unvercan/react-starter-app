{
  "name": "react-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config 'config/webpack/webpack.config.dev.js'",
    "build": "./node_modules/.bin/webpack --config 'config/webpack/webpack.config.prod.js'",
    "lint:check": "./node_modules/.bin/eslint '{src,test}/**/*.{js,jsx,json}' --config 'config/eslint.config.js'",
    "lint:fix": "./node_modules/.bin/eslint '{src,test}/**/*.{js,jsx,json}' --fix  --config 'config/eslint.config.js'",
    "format": "./node_modules/.bin/prettier --write '{src,test}/**/*.{js,jsx,json,css,html}' --config 'config/prettier.config.js'"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unvercanunlu/react-app.git"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "csv-loader": "^3.0.2",
    "editorconfig": "^0.15.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  }
