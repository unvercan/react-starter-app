{
  "name": "react-starter-app",
  "description": "React Starter App",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "keywords": [
    "react",
    "eslint",
    "webpack",
    "prettier",
    "babel",
    "jsx",
    "editorconfig",
    "jest",
    "bootstrap",
    "jquery"
  ],
  "author": {
    "name": "Ünver Can Ünlü",
    "url": "https://unvercanunlu.github.io/"
  },
  "homepage": "https://github.com/unvercanunlu/react-starter-app.git#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/unvercanunlu/react-starter-app.git"
  },
  "bugs": {
    "url": "https://github.com/unvercanunlu/react-starter-app/issues"
  },
  "main": "index.jsx",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config webpack.config/development.js",
    "build": "./node_modules/.bin/webpack --config webpack.config/production.js",
    "lint:check": "./node_modules/.bin/eslint {src,test,config}/**/*.{js,jsx,json} --config .eslintrc.js",
    "lint:fix": "./node_modules/.bin/eslint {src,test,config}/**/*.{js,jsx,json} --fix --config .eslintrc.js",
    "format:check": "./node_modules/.bin/prettier --check {src,test,config}/**/*.{js,jsx,json,css,html} --config prettier.config.js",
    "format:apply": "./node_modules/.bin/prettier --write {src,test,config}/**/*.{js,jsx,json,css,html} --config prettier.config.js",
    "test": "./node_modules/.bin/jest --config jest.config.js"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "csv-loader": "^3.0.2",
    "editorconfig": "^0.15.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  }
}
